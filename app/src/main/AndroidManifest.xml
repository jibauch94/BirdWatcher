<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.jibba.birdwatcher">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".LoginActivity"
            android:label="BirdWatcher" />
        <activity
            android:name=".CreateUserActivity"
            android:label="Create new user" />
        <activity android:name=".MyPageActivity" />
        <activity android:name=".BirdsCatalogActivity" />
        <activity android:name=".BirdsCatalog2Activity" />
        <activity android:name=".CreateObsActivity"></activity>
    </application>

</manifest>